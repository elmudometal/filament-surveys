{
    "name": "elmudodev/filament-surveys",
    "description": "MÃ³dulo de encuestas para Laravel 12 con recursos de Filament.",
    "keywords": [
        "ElmudoDev",
        "laravel",
        "filament",
        "surveys"
    ],
    "homepage": "https://github.com/elmudodev/filament-surveys",
    "support": {
        "issues": "https://github.com/elmudodev/filament-surveys/issues",
        "source": "https://github.com/elmudodev/filament-surveys"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "elmudometal",
            "email": "elmudometal@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.2",
        "filament/filament": "^3.3",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^8.0",
        "nunomaduro/larastan": "^3.0",
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^2.31",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.4",
        "spatie/laravel-ray": "^1.33"
    },
    "autoload": {
        "psr-4": {
            "ElmudoDev\\FilamentSurveys\\": "src/",
            "ElmudoDev\\FilamentSurveys\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ElmudoDev\\FilamentSurveys\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ElmudoDev\\FilamentSurveys\\FilamentSurveysServiceProvider"
            ],
            "aliases": {
                "FilamentSurveys": "ElmudoDev\\FilamentSurveys\\Facades\\FilamentSurveys"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
